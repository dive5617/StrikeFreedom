<Window x:Class="pBuild.MGF_PSM_Filter_Dialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MGF_psm_filter_dialog" Height="300" Width="300" Icon="pBuild.ico">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"></RowDefinition>
            <RowDefinition Height="Auto"></RowDefinition>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"></ColumnDefinition>
            <ColumnDefinition Width="*"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <TextBlock Grid.Row="0" Grid.Column="0" Margin="3">Title</TextBlock>
        <TextBox Grid.Row="0" Grid.Column="1" Margin="3" x:Name="title_subStr_txt" Text="" KeyDown="enter_keyDown">
            <TextBox.ToolTip>
                <TextBlock>Filter By Title SubString</TextBlock>
            </TextBox.ToolTip>
        </TextBox>
        <Button Grid.Row="1" Grid.Column="1" Click="filter_btn_clk">Filter</Button>
    </Grid>
</Window>
